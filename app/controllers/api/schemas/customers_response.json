{
  "$id": "customers_response.json",
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "customers"
    ],
  "properties": {
    "customers": {
      "type": "array",
      "items": {
        "required": [
          "first_name",
          "last_name"
        ],
        "properties": {
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "company": {
            "type": "object",
            "required": [
              "company_name"
            ],
            "properties": {
              "company_name": {
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}
